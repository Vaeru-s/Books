<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{{title}}{% endblock %}</title>
        {% block stylesheets %}
             <link rel="stylesheet" href="/build/app.css">
        {% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
                <script src="/build/app.js"></script>
                <script src="/build/runtime.js"></script>
              
    <script>
        $('#description a').click(function(e){
            e.preventDefault();
            $(this).tab('show');
        });
        $('#characteristics a').click(function(e){
            e.preventDefault();
            $(this).tab('show');
        });
    </script>
       
        {% endblock %}
        
    </body>
</html>
